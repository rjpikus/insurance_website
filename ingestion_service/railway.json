{
    "$schema": "https://railway.com/railway.schema.json",
    "build": {
      "buildCommand": "docker build -t ingestion-service ."
      },
    "deploy": {
      "startCommand": "gunicorn app.main:app -b 0.0.0.0:$PORT --workers 2",
